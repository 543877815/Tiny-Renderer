  {
    "object": "basic_light",
    "programs": [
      {
        "vao": [ "cubeVAO" ],
        "vbo": [ "cude" ],
        "vertex_shader": "./shader/rect_vs.glsl",
        "fragment_shader": "./shader/rect_fs.glsl",
        "uniform": [ "projection", "view", "model" ]
      }
    ],
    "VAOs": [
      {
        "name": "lightCubeVAO",
        "vertex_info": [
          {
            "attriName": "aPos",
            "attriLocation": 0,
            "count": 3,
