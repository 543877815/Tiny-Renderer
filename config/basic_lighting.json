          -0.5,
          -0.5,
          0.0, // bottom left
          -0.5,
          0.5,
          0.0 // top left 
        ],
        "indices_data": [
          0,
          1,
          3,
          1,
          2,
          3
        ]

      }
    ],
    "vertex_shader": "./shader/rect_vs.glsl",
    "fragment_shader": "./shader/rect_fs.glsl",
    "uniform": [ "projection", "view", "model" ]
  }
